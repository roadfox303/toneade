<% if user_signed_in? %>
  <%= link_to "New Phrase", new_blog_path, data: { "turbolinks"=> false }, class:"btn_create" %>
<% end %>
<% unless @blogs.empty?%>
  <%= render partial: 'shared/phrase_list', locals: { list: @blogs, list_id:'all_blog', title: '投稿フレーズ一覧' } %>
  <%= paginate @blogs %>
<% end %>
