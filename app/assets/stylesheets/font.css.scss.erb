@charset "UTF-8";

@font-face {
    font-family: 'toneade-font';
    src: url('<%= asset_path('toneade-font.eot') %>');
    src: url('<%= asset_path('toneade-font.eot') + '?#iefix' %>') format('embedded-opentype'),
         url('<%= asset_path('toneade-font.woff') %>') format('woff'),
         url('<%= asset_path('toneade-font.ttf') %>') format('truetype'),
         url('<%= asset_path('toneade-font.svg') + '#toneade-font' %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
